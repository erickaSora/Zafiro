                                          </ImageView>
                                       </children>
                                    </Pane>
                                    <TableView fx:id="discounts" maxHeight="90.0" maxWidth="325.0" onMouseClicked="#loadSelectedItemModal" prefHeight="139.0" prefWidth="331.0">
